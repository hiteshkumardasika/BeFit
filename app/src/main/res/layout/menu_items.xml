<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circle="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/calendar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:elevation="2dp"
        android:src="@drawable/run"
        circle:name="@string/calendar" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/jeffrey"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/jeff_round"
        circle:name="JEFFREY" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/shakti"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:elevation="2dp"
        android:src="@drawable/shakti_round"
        circle:name="SHAKTI" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/jay"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:elevation="2dp"
        android:src="@drawable/jay_round"
        circle:name="JAYENDRA" />

</merge>
